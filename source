local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Relix Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "Relix Hub"})

-- Values

_G.AutoSword = true
_G.AutoDeer = true
_G.AutoBoost = true
_G.autolvl = true


--Functions

function Deerspam()
    while _G.AutoDeer == true do
        local args = {
            [1] = "deer",
            [2] = 0
        }

        game:GetService("ReplicatedStorage").Player.Module.christmas.ch_pur_Event:FireServer(unpack(args))
        wait()
    end

end

function supersword()
    while _G.AutoSword == true do
        local args = {
            [1] = "sword",
            [2] = 0
        }

        game:GetService("ReplicatedStorage").Player.Module.christmas.ch_pur_Event:FireServer(unpack(args))
        wait()
    end

function boost()
    while _G.AutoBoost == true do
        local args = {
            [1] = "lucky3x",
            [2] = 0
        }

        game:GetService("ReplicatedStorage").Player.Module.christmas.ch_pur_Event:FireServer(unpack(args))

        local args = {
            [1] = "lucky2x",
            [2] = 0
        }

        game:GetService("ReplicatedStorage").Player.Module.christmas.ch_pur_Event:FireServer(unpack(args))

        local args = {
            [1] = "coin",
            [2] = 0
        }

        game:GetService("ReplicatedStorage").Player.Module.christmas.ch_pur_Event:FireServer(unpack(args))

        local args = {
            [1] = "exp",
            [2] = 0
        }

        game:GetService("ReplicatedStorage").Player.Module.christmas.ch_pur_Event:FireServer(unpack(args))
        wait()
    end

end

end

local args = {
    [1] = 1,
    [2] = "m6_6"
}

game:GetService("ReplicatedStorage").Player.Module.Mission.reward_missionEvent:FireServer(unpack(args))

local args = {
    [1] = game:GetService("Players").LocalPlayer
}

game:GetService("ReplicatedStorage").Player.Event.lvup:FireServer(unpack(args))




